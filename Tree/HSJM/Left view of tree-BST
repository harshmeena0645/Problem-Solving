Left view of the tree

Input:
6
10 5 15 2 7 12
Output:
10
5
2

Input:
7
20 10 5 15 25 30 3
Output:
20
10
5
3



class Node:
    def __init__(self,data):
        self.data=data
        self.left=None
        self.right=None

def left_view(root):
    if not root:
        return None
    print(root.data)
    temp=root
    while temp.left:
        print(temp.left.data)
        temp=temp.left
    
def create_BST(values):
    if not values:
        return None
    root=None
    for v in values:
        if v is not None:
            root=insert_values(root,v)
    return root

def insert_values(root,v):
    if root is None:
        return Node(v)
    if v < root.data:
        root.left=insert_values(root.left,v)
    elif v > root.data:
        root.right=insert_values(root.right,v)
    return root

values=[20,10,"null",15,25,30,3]
values=[None if v=="null" else v for v in values]
root=create_BST(values)
left_view(root)
