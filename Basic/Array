9ï¸âƒ£ Check Prime Number
def is_prime(num):
    if num < 2:
        return False
    for i in range(2, int(num**0.5) + 1):
        if num % i == 0:
            return False
    return True

ðŸ”Ÿ Factorial of a Number
def factorial_rec(n):
    if n < 0:
        raise ValueError
    if n in (0, 1):
        return 1
    return n * factorial_rec(n - 1)

import math
print(math.factorial(5))  # 120

