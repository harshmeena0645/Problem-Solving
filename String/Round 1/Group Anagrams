Group Anagrams

strs = ["eat","tea","tan","ate","nat","bat"]
Output: [["bat"],["nat","tan"],["ate","eat","tea"]]

strs = [""]
Output: [[""]]

strs = ["a"]
Output: [["a"]]



def val(s):
    d = {}
    for word in s:
        key = ''.join(sorted(word))
        if key in d:
            d[key].append(word)
        else:
            d[key] = [word]
    res = sorted(d.values(), key=lambda x: len(x))
    return res

s = ["eat","tea","tan","ate","nat","bat"]
print(val(s))
