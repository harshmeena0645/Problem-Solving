Valid Parentheses
 

s = "()"
Output: true

s = "()[]{}"
Output: true

s = "(]"
Output: false

s = "([])"
Output: true


class Solution(object):
    def isValid(self, s):
        d = {'(': ')', '[': ']', '{': '}'}
        stack = []
        for char in s:
            if char in d:
                stack.append(char)
            else:
                if not stack or d[stack.pop()] != char:
                    return False
        return not stack

s = input()
solution = Solution()
print(solution.isValid(s))
